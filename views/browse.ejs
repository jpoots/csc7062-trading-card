<!DOCTYPE html>
<html lang="en">
<head>
    <%- include("partials/head") %>
</head>
<body>
    <%- include("partials/navbar") %>
    <div class="container browsecontainer">

        <% cards.forEach(row => { %>
            <div class="row browserow">
                <% row.forEach(card => { %>
                    <div class="column browsecol">
                        <div class="card">
                            <a href="card" class="cardlink">
                                <img src="<%= card.image %>" alt="" width="100%" class="cardimg">
                                <h6 class="centertext browsetitle"><%= card.name %></h6>
                            </a>
                        </div>
                    </div>
                <% }) %>
            </div>
        <% }) %>
    </div>
    <%- include("partials/footer") %>
</body>
</html>